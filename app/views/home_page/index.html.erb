Feature Boats:

<div class="boat_panel" >
    <% @boats.limit(6).each_slice(3).each do |group| %>
      <% group.each do |boat| %>
          <% if boat.id !=nil %>
            <%= link_to image_tag(boat.boat_attachments[0].image.url, :width=>"200"), boat %>
          <% end %>
      <% end %>
     <br>
    <% end %>
</div>